{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "eslint-config-airbnb-base"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": ["error", "windows"],
        "indent": ["error", 4],
        "lines-between-class-members": ["error", "always", { "exceptAfterSingleLine": true }],
        "import/prefer-default-export": "off",
        "no-undef": "warn",
        "no-unused-vars": "warn",
        "max-len": ["error", { "code": 120 }]
    }
}
